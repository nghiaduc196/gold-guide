{% if page.header %}
  <nav class="navbar navbar-expand-lg navbar-light bg-header py-1 py-md-2 position-sticky top-0 z-index-9999">
    {{ page.header }}
  </nav>
{% endif %}


{% block main %}
  <main role="main">
    <div class="container">
      {{ page.content }}
    </div>
  </main>
{% endblock %}
<footer class="site-footer">
  <div class="container">
    {{ page.page_bottom }}
  </div>
</footer>
